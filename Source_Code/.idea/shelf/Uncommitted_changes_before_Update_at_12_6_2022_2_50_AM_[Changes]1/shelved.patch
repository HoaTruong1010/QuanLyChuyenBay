+from flask import render_template, request, redirect, session, jsonify
+from FlightManagement import app, dao, utils
+from flask_login import login_user, logout_user, login_required
+from FlightManagement.decorators import anonymous_user
+# import cloudinary.uploader
+
+
+def index():
+    airports = dao.load_airports()
+    airlines = dao.load_from_airports(airport_id=request.args.get("airport_id"),
+                                 kw=request.args.get('keyword'))
+    return render_template('index.html', airlines=airlines, airports=airports)
+
+
+def login_admin():
+    username = request.form['username']
+    password = request.form['password']
+
+    user = dao.auth_user(username=username, password=password)
+    if user:
+        login_user(user=user)
+
+    return redirect('/admin')
+
+
+def booking(airport_id):
+    a = dao.get_airline_by_id(airport_id)
+    return render_template('booking.html', airport=a)
+
+
+def booking_ticket():
+    return render_template('booking.html')
